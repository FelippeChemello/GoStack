{
    "version": 2,
    "name": "gobarber-backend",
    "builds": [
        {
            "src": "dist/*.js",
            "use": "@now/node"
        }
    ],
    "routes": [{
            "src": "/(.*)",
            "dest": "/dist/server.js"
        }
    ],
    "functions": {
        "compileTypescript.js" : {
            "runtime": "@vercel/node"
        }
    }
}
